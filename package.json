{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --exec 'remix build && ts-node entrypoints/server.ts' --watch entrypoints/server.ts",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:graphile-worker": "NODE_ENV=development ts-node entrypoints/graphile-worker.server.ts",
    "dev-prebuild": "run-p \"dev-prebuild:*\"",
    "dev-prebuild:css": "tailwindcss -i ./styles/app.css -o app/styles/app.css",
    "start": "cross-env NODE_ENV=production ts-node entrypoints/server.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "postinstall": "ops/bin/override-prisma-types.sh"
  },
  "dependencies": {
    "@prisma/client": "4.2.1",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@sinclair/typebox": "^0.24.43",
    "@temporalio/client": "^1.4.4",
    "@temporalio/worker": "^1.4.4",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.2",
    "express-openid-connect": "^2.8.0",
    "firecracker-client": "./deps/firecracker-client",
    "flowbite": "^1.5.2",
    "flowbite-react": "^0.1.10",
    "graphile-worker": "^0.13.0",
    "http-status-codes": "^2.2.0",
    "netmask": "^2.0.2",
    "node-ssh": "^13.0.0",
    "prisma": "4.2.1",
    "proper-lockfile": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tail": "^2.2.4",
    "typed-inject": "^3.0.1",
    "undici": "^5.11.0",
    "winston": "^3.8.1",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@swc/core": "^1.2.241",
    "@swc/helpers": "^0.4.7",
    "@swc/jest": "^0.2.22",
    "@temporalio/testing": "^1.4.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/netmask": "^1.0.30",
    "@types/node": "16.18",
    "@types/pg": "^8.6.5",
    "@types/proper-lockfile": "^4.1.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/ssh2": "^1.11.6",
    "@types/ssh2-streams": "^0.1.9",
    "@types/tail": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
