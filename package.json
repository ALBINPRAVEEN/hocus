{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --exec 'remix build && ts-node server.ts' --watch ./server.ts",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production ts-node ./server.ts",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "4.2.1",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express-openid-connect": "^2.8.0",
    "flowbite": "^1.5.2",
    "flowbite-react": "^0.1.10",
    "http-status-codes": "^2.2.0",
    "prisma": "4.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "superjson": "^1.9.1",
    "typed-inject": "^3.0.1",
    "winston": "^3.8.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@swc/core": "^1.2.241",
    "@swc/helpers": "^0.4.7",
    "@swc/jest": "^0.2.22",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-plugin-filename-rules": "^1.2.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.3",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14"
  }
}
