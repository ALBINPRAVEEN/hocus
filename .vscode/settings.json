{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "files.exclude": {
    ".cache/": true,
    "node_modules/": true,
    "build/": true,
    "vscode_extension": true
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "autoOpenWorkspace.enableAutoOpenIfSingleWorkspace": true,
}